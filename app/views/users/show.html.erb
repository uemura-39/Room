<div class="contents row">
  <%= link_to "ログアウト", destroy_user_session_path, class:'header--post', method: :delete %> 
  <p><%= @nickname %>さんの投稿一覧</p>

  <% @posts.each do |post| %>
    <div class="content_post" style="background-image: url(<%= post.image %>);">
      <%= simple_format(post.text) %>
      
    </div>
  <% end %>
</div>
